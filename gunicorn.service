[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/var/lib/jenkins/workspace/Django_app/blogproj
ExecStart=/var/lib/jenkins/workspace/Django_app/venv/bin/gunicorn --access-logfile /var/lib/jenkins/workspace/Django_app/logs/error.log - --workers 3 --bind unix:/run/gunicorn.sock myproject.wsgi:application

[Install]
WantedBy=multi-user.target